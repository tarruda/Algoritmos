import "layout.stg"

@page.title() ::= "Login"

@page.menu() ::= ""

@page.headerButton() ::= ""

@page.main() ::= <<
<div id="login-container">
	<form id="login-form" action="login" method="post">	
		<div data-role="fieldcontain">		
			<label for="userName">Qual o No da sua mesa?</label><input type="text" id="userName" name="userName" />					
		</div>			
		<div data-role="fieldcontain">
			<input type="submit" value="Entrar" />
		</div>			
	</form>
</div>
>>